{% extends "base.html.twig" %}
{%  block body %}
    <div class="row">
        <div class="col-sm-4"></div>
        <div class="col-sm-4">
            <div class="panel panel-primary">
                <div class="panel-heading text-center"><h4>Artist(s) in this show</h4></div>
                <div class="panel-body">

                    {%  for artist in artists %}
                        <div class="row">
                            <div class="col-sm-2"></div>
                            <div class="col-sm-9">
                                {{ artist.lastName }}, {{ artist.firstName }}
                            </div>
                        </div>
                    {% endfor %}
                </div></div></div></div>
            {% endblock body %}
